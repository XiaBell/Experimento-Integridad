# Archivo de ejemplo de variables de Terraform
# Copiar este archivo a terraform.tfvars y completar con tus valores

aws_region = "us-east-1"
project_name = "experimento-integridad"
instance_type = "t3.micro"

# IMPORTANTE: Cambiar por el nombre de tu clave SSH en AWS
key_name = "tu-clave-ssh-aqui"

# IMPORTANTE: Cambiar por contraseñas seguras
db_password = "tu-contraseña-segura-aqui"
jwt_secret_key = "tu-clave-secreta-jwt-aqui"

# Restringir acceso SSH a tu IP (opcional, para mayor seguridad)
# allowed_cidr_blocks = ["TU.IP.PUBLICA.AQUI/32"]

